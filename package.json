  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development node server.js",
    "prod": "next build",
    "start": "NODE_ENV=production node server.js",
    "deploy": "rm -rf .next && npm run prod && gcloud app deploy",
    "lint": "eslint 'pages/**/*.{js,ts,tsx}'",
